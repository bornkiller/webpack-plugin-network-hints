{
  "name": "@coco-platform/network-hints",
  "version": "0.4.0",
  "description": "Inject network hints automatic, cooperate with html-webpack-plugin.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prepublish": "npm-run-all -s clean compile:native compile:cjs",
    "test": "jest --coverage",
    "test:cov": "jest --watch",
    "clean": "rm -rf lib native",
    "compile": "babel src --source-maps --out-dir lib --copy-files"
  },
  "files": [
    "lib",
    "native"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coco-platform/network-hints.git"
  },
  "keywords": [
    "nodejs",
    "universal"
  ],
  "author": "huang.jian <hjj491229492@hotmail.com>",
  "contributors": [
    {
      "name": "huang.jian",
      "email": "hjj491229492@hotmail.com"
    },
    {
      "name": "yang.yuncai",
      "email": "383235388@qq.com"
    }
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coco-platform/network-hints/issues"
  },
  "homepage": "https://github.com/coco-platform/network-hints#README",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.6",
    "@types/minimatch": "^3.0.3",
    "@types/node": "^11.9.5",
    "@types/webpack": "^4.4.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0"
  },
  "dependencies": {
    "minimatch": "^3.0.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "html-webpack-plugin": "^3.2.0"
  }
}
